{
  "manifest_version": 2,
  "name": "Amat.su/kaze!",
  "version": "0.1",
  "browser_action": {
    "default_icon": "img/icon2.png",
    "default_popup": "html/menu.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/jquery.min.js", "js/content.js"]
    }
  ]
}